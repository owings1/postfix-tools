{
  "pmapfiles": [
    "client_checks",
    "sender_checks",
    "local_dsn_filter",
    "virtual"
  ],
  "ignorekeys": [
    "maillog_file"
  ],
  "auth": {
    "dir": "/etc/auth",
    "alg": "SHA512-CRYPT",
    "uid": 600,
    "gid": 515,
    "pwgenopts": "-scnyBr='\"\\`|}{/",
    "emaildir": "/var/mail"
  }
}