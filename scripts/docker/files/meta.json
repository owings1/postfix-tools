{
  "hashfiles": [
    "client_checks",
    "sender_checks",
    "virtual"
  ],
  "ignorekeys": [
    "maillog_file"
  ],
  "auth": {
    "file": "/etc/auth/passwd",
    "alg": "SHA512-CRYPT",
    "group": "email",
    "gid": 515,
    "pwgenopts": "-scnyBr='\"\\`|}{/",
    "emaildir": "/home/email",
    "shell": "/usr/sbin/nologin"
  }
}